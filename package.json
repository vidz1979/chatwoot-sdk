{
  "name": "@jrvidotti/chatwoot-sdk",
  "version": "1.2.5",
  "description": "SDK for JS/Typescript for Chatwoot.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vidz1979/chatwoot-sdk.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "format": "npm exec -- prettier --write ./src --loglevel warn",
    "lint": "npm exec -- eslint ./src --ext .ts --fix",
    "build": "npm exec -- tsc -b --clean && npm exec -- tsc --declaration",
    "test": "echo \"Error: no test specified\" && exit 0",
    "release": "npm exec -- release-it"
  },
  "keywords": [
    "chatwoot"
  ],
  "author": "Figuro <engineering@figuro.la> (https://figuro.la)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "prettier": "^2.7.1",
    "release-it": "^15.4.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "axios": ">=0.27 < 2"
  }
}